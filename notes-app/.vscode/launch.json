{
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Node.js - Debug Current File",
                "type": "node",
                "request": "launch",
                "program": "${file}",
                "args": "${input:fileArgs}"
            }
        ]
    },
    "inputs": [
        {
            "id": "fileArgs",
            "type": "promptString",
            "description": "args for running file",
            "default": ""
        }
    ]
}
